#include <stdio.h>

int main() { 
    int a=4003,i,pin;
    for(i=1;i<=3;i++)
    {
        printf("enter the pin\t");
        scanf("%d",&pin);
        
        if(a==pin)
        {
            printf("collect your cash");
            break;
        }
        else if(i==3)
        {
            printf("your Card is Blocked");
        }
        else if(a!=pin)
        {
            printf("incorrect pin\n"); 
        }

    }


    return 0;
}
